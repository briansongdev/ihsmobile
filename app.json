{
  "expo": {
    "name": "IHS Students",
    "slug": "ihsmobile",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "cover",
      "backgroundColor": "#e6fef9"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "infoPlist": {
        "NSFaceIDUsageDescription": "Use of Face ID authentication is to allow user to conveniently and privately access sensitive information."
      },
      "bundleIdentifier": "com.briansapps.ihsmobile"
    },
    "android": {
      "googleServicesFile": "./google-services.json",
      "adaptiveIcon": {
        "foregroundImage": "./assets/icon.png",
        "backgroundColor": "#e6fef9"
      },
      "package": "com.briansapps.ihsmobile"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "da381f5b-a97a-4262-94db-cbeac2a78f91"
      }
    }
  }
}
